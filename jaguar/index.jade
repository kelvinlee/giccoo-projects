extends ../libs/jade/layout
block title
	| 捷豹汽车
block css
	link(rel="stylesheet",href="http://disk.giccoo.com/projects/libs/css/animate.min.css",media="none",onload="if(media!='all')media='all'")
	link(rel="stylesheet",href="css/base.css")

block content
	- var images = "http://image.giccoo.com/projects/jaguar/img"
	// - var images = "img"
	.wechat-share-hide
		//- img#share(src="http://disk.giccoo.com/projects/dazhong/img/share.jpg")
	.main
		.homepage
			.bg
				img(src="#{images}/banner.jpg")
				a.outlink(href="http://www.jaguar.com.cn/art-of-performance.html")
			.title
				img(src="#{images}/title.png")
			.bg
				.form-box
					//- .title
						img(src="#{images}/title-form.png")
					register(action="http://api.giccoo.com/jaguar_201602/insert/")
			.title
				img(src="#{images}/title-2.png")
			.bg.last
				.content
					//- .title
						img(src="#{images}/title.png")
					.sliders
						slider(myid="secondSlider",callback="changePoint",list="#{images}/car-1.jpg,#{images}/car-2.jpg,#{images}/car-3.jpg,#{images}/car-4.jpg,#{images}/car-5.jpg",link="http://www.jaguar.com.cn/jaguar-range/xe/index.html,http://www.jaguar.com.cn/jaguar-range/2016-xf/index.html,http://www.jaguar.com.cn/jaguar-range/xj/index.html,http://www.jaguar.com.cn/jaguar-range/f-type/index.html,http://www.jaguar.com.cn/jaguar-range/f-pace/index.html")
					.points
						span.on XE
						span XF
						span XJ
						span F-TYPE
						span F-PACE

		//- .backTop
			img(src="#{images}/back-top.png")
