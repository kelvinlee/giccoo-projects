extends ../libs/jade/layout-640
block title
	| 致趣实验室
block css
	script(src="/libs/js/viewport.js")
	link(rel="stylesheet",href="/libs/css/animate.min.css",media="none",onload="if(media!='all')media='all'")
	link(rel="stylesheet",href="css/base.css")

block content
	// - var images = "http://disk.giccoo.com/projects/huaweiG7/img"
	- var images = "img"
	.wechat-share-hide
		//- img#share(src="http://disk.giccoo.com/projects/huaweiG7/img/share.jpg")
	//- gif(width="640",height="1134",normal='[0,86, "replay"]',replay='[87, 126]',stop="86",play="normal",count="127",src="img/animate/brands/brand-id.png")
	//- gif(id="brandbg",width="640",height="1134",normal='[0,6, "stop"]',replay='[0,7]',stop="[7,7]",play="normal",count="8",src="img/animate/brandbg/brand-id.png")
	//- animations='{ "normal": [0,86, "replay"], "replay": [87, 126], "stop": 86 }'
	.main
		.homepage
			.bottle.bottle-logo
				img(src="#{images}/bottle-logo.png")
				gif(id="bottlelogo",width="480",height="620",count="63",normal='0',replay='[0,62,"stop"]',stop="62",play="normal",src="img/animate/bottle-logo/logo-id.png")
				.on-button(onclick="openBottleMain(this)",page-name="logo")
				//- .white
					img(src="#{images}/bottle-logo-white.png")
				//- .newbottle
					img(src="#{images}/bottle-logo-full.png")
					.white.white-1
						img(src="#{images}/bottle-logo-full-white-1.png")
					.white.white-2
						img(src="#{images}/bottle-logo-full-white-2.png")
			.bottle.bottle-strategy
				img(src="#{images}/bottle-strategy.png")
				gif(id="bottlestrategy",width="850",height="200",count="78",normal='0',replay='[0,77,"stop"]',stop="77",play="normal",src="img/animate/bottle-strategy/strategy-id.png")
				.shadow
					img(src="#{images}/bottle-strategy-shadow.png")
				.on-button(onclick="openBottleMain(this)",page-name="strategy")
				//- .white
					img(src="#{images}/bottle-strategy-white.png")
			.bottle.bottle-technology
				img(src="#{images}/bottle-technology.png")
				gif(id="bottletechnology",width="800",height="260",count="60",normal='0',replay='[0,59,"stop"]',stop="59",play="normal",src="img/animate/bottle-technology/technology-id.png")
				.shadow
					img(src="#{images}/bottle-technology-shadow.png")
				.on-button(onclick="openBottleMain(this)",page-name="technology")
				//- .white
					img(src="#{images}/bottle-technology-white.png")
			.bottle.bottle-brand
				img(src="#{images}/bottle-brand.png")
				gif(id="bottlebrand",width="300",height="750",count="51",normal='0',replay='[0,50,"stop"]',stop="50",play="normal",src="img/animate/bottle-brand/brand-id.png")
				.on-button(onclick="openBottleMain(this)",page-name="brand")
				//- .white
					img(src="#{images}/bottle-brand-white.png")
				.bubbles
					-for (var i=0;i<3;i++)
						.bubble(class="bubble-#{i+1}")
							img(src="#{images}/homepage-bubble.png")
			.bottle.bottle-media
				img(src="#{images}/bottle-media.png")
				gif(id="bottlemedia",width="380",height="380",count="76",normal='0',replay='[0,75,"stop"]',stop="75",play="normal",src="img/animate/bottle-media/media-id.png")
				.shadow
					img(src="#{images}/bottle-media-shadow.png")
				.on-button(onclick="openBottleMain(this)",page-name="media")
				//- .white
					img(src="#{images}/bottle-media-white.png")
			.bottle.bottle-contactus
				//- img(src="#{images}/bottle-contactus.png")
				gif(id="bottlecontactus",width="480",height="480",count="82",normal='0',replay='[0,81,"stop"]',stop="81",play="normal",src="img/animate/bottle-contactus/contactus-id.png")
				.shadow
					img(src="#{images}/bottle-contactus-shadow.png")
				.on-button(onclick="openBottleMain(this)",page-name="contactus")
				//- .white
					img(src="#{images}/bottle-contactus-white.png")
			.bottle.bottle-award
				//- img(src="#{images}/bottle-award.png")
				gif(id="bottleaward",width="600",height="300",count="82",normal='0',replay='[0,81,"stop"]',stop="81",play="normal",src="img/animate/bottle-award/award-id.png")
				.shadow
					img(src="#{images}/bottle-award-shadow.png")
				.on-button(onclick="openBottleMain(this)",page-name="award")
				//- .white
					img(src="#{images}/bottle-award-white.png")
			.bottle.middle-tower
				img(src="#{images}/middle-tower.png")
			.bottle.left-tower
				img(src="#{images}/search-tower.png")
			.bottle.right-tower
				img(src="#{images}/time-tower.png")
				.timer
					img(src="#{images}/right-tower-timer.png")
					timer
				//- playsound(src="mp3/1.mp3")
			.bottle.last-bottle
				img(src="#{images}/bottom-bottle.png")
			.bottle.bottle-fire-1
				img(src="#{images}/bottle-lighter.png")
				.fire
					img(src="#{images}/bottle-lighter-fire.png")
			.bottle.bottle-fire-2
				img(src="#{images}/bottle-lighter.png")
				.fire
					img(src="#{images}/bottle-lighter-fire.png")
			.bottle.bottle-search.tada.animated.infinite.duration-15
				img(src="#{images}/magn.png")

		.pages-technology
			.content(onclick="backBottleMain('technology')")
				.m-top
					.bg
						img(src="#{images}/pages-technology-top.png")
				.m-bottom
					.bg
						img(src="#{images}/pages-technology-bottom.png")
		.pages-award
			.bg
				img(src="#{images}/pages-award-bg.png")
			.content(onclick="backBottleMain('award')")
				.icons.icons-1
					-for (var i=1;i<3;i++)
						.icon(rel="#{i}")
							img(src="#{images}/pages-award-icon-#{i}.png")
				.icons.icons-2
					-for (var i=3;i<6;i++)
						.icon(rel="#{i}")
							img(src="#{images}/pages-award-icon-#{i}.png")
				.icons.icons-3
					-for (var i=6;i<8;i++)
						.icon(rel="#{i}")
							img(src="#{images}/pages-award-icon-#{i}.png")
				.other.other-1
					img(src="#{images}/pages-award-other-1.png")
				.other.other-2
					img(src="#{images}/pages-award-other-2.png")
		.pages-media
			.bg
				img(src="#{images}/pages-media-bg.png")
			.content(onclick="backBottleMain('media')")
				.icons.icons-1
					-for (var i=1;i<7;i++)
						.icon(rel="#{i}")
							img(src="#{images}/pages-media-icon-#{i}.png")
				.icons.icons-2
					-for (var i=7;i<13;i++)
						.icon(rel="#{i}")
							img(src="#{images}/pages-media-icon-#{i}.png")
				.bottles.bottle-1
					-for (var i=1;i<5;i++)
						.icon
							img(src="#{images}/pages-media-bottle-#{i}.png")
				.bottles.bottle-2
					-for (var i=5;i<8;i++)
						.icon
							img(src="#{images}/pages-media-bottle-#{i}.png")
		.pages-logo
			.bg
			.content(onclick="backBottleMain('logo')")
				.logo-page-bottle
				.bottle.bottle-logo-next
					img(src="#{images}/bottle-logo-next.png")
					.text.fadeIn.animated.delay-20
						img(src="#{images}/pages-logo-text.png")
					.white-1
						img(src="#{images}/bottle-logo-next-white-1.png")
					.white-2
						img(src="#{images}/bottle-logo-next-white-2.png")
					.white-3
						img(src="#{images}/bottle-logo-next-white-3.png")
					.white-4
						img(src="#{images}/bottle-logo-next-white-4.png")

				//- .bottle.bottle-logo-next
					img(src="#{images}/bottle-logo-next.png")
					.white
						img(src="#{images}/bottle-logo-next-white.png")
		.pages-brand
			.bg
				gif(id="brandbg",rx="0",ry="0",width="640",height="1134",normal='[0,6, "replay"]',replay='7',stop="0",play="normal",count="8",src="img/animate/brandbg/brand-id.png")
			.content(onclick="backBottleMain('brand')")
				gif(id="brand",rx="0",ry="0",width="640",height="1134",normal='[0,86, "replay"]',replay='[87, 126]',stop="127",play="normal",count="127",src="img/animate/brands/brand-id.png")
				.ctrls
					.item.item-1(rel="1")
					.item.item-2(rel="2")
					.item.item-3(rel="3")
		.pages-strategy
			.content(onclick="backBottleMain('strategy')")
				.m-top
					img(src="#{images}/pages-strategy-m-top.png")
					.item-logo
						img(src="#{images}/pages-strategy-logo.png")
					.item-switch
						img(src="#{images}/pages-strategy-switch.png")
					.item-light-red
						img(src="#{images}/pages-strategy-light-red.png")
					.item-light-blue
						img(src="#{images}/pages-strategy-light-blue.png")
					.item-light-all
						img(src="#{images}/pages-strategy-light-all.png")
					.item-icloud-1
						img(src="#{images}/pages-strategy-icloud-1.png")
					.item-icloud-2
						img(src="#{images}/pages-strategy-icloud-2.png")
					.item-icons
						img(src="#{images}/pages-strategy-icons.png")
				.m-bottom
					.line1
					.line2
					.p1
						img(src="#{images}/pages-strategy-people-1.png")
					.p2
						img(src="#{images}/pages-strategy-people-2.png")
					.parts1
						img(src="#{images}/pages-strategy-parts-red.png")
					.parts2
						img(src="#{images}/pages-strategy-parts-yellow.png")
					.parts3
						img(src="#{images}/pages-strategy-parts-roulette.png")
					.fcanvas
						.items
							.item
								img(src="#{images}/pages-strategy-canvas-1.png")
							.item
								img(src="#{images}/pages-strategy-canvas-2.png")
					.bg
						img(src="#{images}/pages-strategy-m-bottom.png")
		.pages-contactus
			.content(onclick="backBottleMain('contactus')")
				.text
					img(src="#{images}/pages-contactus-text.png")
				.qrcode
					img(src="#{images}/pages-contactus-qrcode.png")
				//- .vitro
					img(src="#{images}/pages-contactus-vitro.png")
				.drops
					img(src="#{images}/pages-contactus-drops.png")
				.bubbles
					-for (var i=0;i<10;i++)
						.bubble(class="bubble-#{i+1}")
							img(src="#{images}/homepage-bubble.png")
				.music
					.c1
					.c2
					.c3
					.line

				.pointer
					img(src="#{images}/pages-contactus-pointer.png")
				.bg
					img(src="#{images}/pages-contactus-bg.png")

