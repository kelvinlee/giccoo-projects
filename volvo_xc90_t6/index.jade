block content
	//- var images = "http://disk.giccoo.com/projects/volvo_xc90_201601/img"
	- var images = "img"
	.wechat-share-hide
		//- img#share(src="http://disk.giccoo.com/projects/dazhong/img/share.jpg")
	.main
		.homepage
			.bg
				img(src="#{images}/banner.jpg")
			.bg
				img(src="#{images}/form-bg.jpg")
				.form-box
					register(action="http://api.giccoo.com/volvo_xc90_t6/insert/")
				.submit#submit
					img(src="#{images}/form-submit.jpg")
			.bg
				img(src="#{images}/info.jpg")
			.bg.slider-bg
				.sliders
					slider(myid="secondSlider",callback="changePoint",list="#{images}/car-1.jpg,#{images}/car-2.jpg,#{images}/car-3.jpg,#{images}/car-4.jpg,#{images}/car-5.jpg,#{images}/car-6.jpg")
					
			.bg
				img(src="#{images}/last.jpg")
				.points
					span.on
					span
					span
					span
					span
					span
					span
		.backTop
			img(src="#{images}/back-top.png")
