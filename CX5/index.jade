extends ../libs/jade/layout-clean
block title
	| 第25小时的精彩


block css
	meta(name="author",content="m.giccoo.com")
	link(rel="stylesheet",href="http://image.giccoo.com/projects/libs/css/animate.min.css",media="none",onload="if(media!='all')media='all'")
	link(rel="stylesheet",href="css/base.css?v=1")

block content
	- url = "http://image.giccoo.com/projects/CX5/"
	//- url = "./"
	
	.wechat-share-hide
		img#share(src="http://image.giccoo.com/projects/CX5/img/share.jpg")
	.pages
		.product-logo
			img(src="#{url}img/product-logo.png")
		.page.page-homepage
			.content
				.text.text-1.fadeInUp.animated.delay-5
					img(src="#{url}img/homepage-text-1.png")
				.text.text-2.fadeInUp.animated.delay-9
					img(src="#{url}img/homepage-text-2.png")
				.text.text-3.fadeInUp.animated.delay-10
					img(src="#{url}img/homepage-text-3.png")
				.text.text-4.fadeInUp.animated.delay-11
					img(src="#{url}img/homepage-text-4.png")
				.btn.btn-1.fadeInUp.animated.delay-13(next-page=".page-contents")
					img(src="#{url}img/homepage-btn-1.png")
				.car.fadeInUp.animated.duration-15
					img(src="#{url}img/homepage-car.png")
		
		.page.page-contents.on
			.content
				.navbar
					.btn.btn-register
						img(src="#{url}img/listpage-btn.png")
				.texts
					.text.text-1.fadeInDown.animated.delay-2
						img(src="#{url}img/listpage-text-1.png")
						.title 
						.btn.btn-1(next-page=".page-content",data-content='[{"title":"唤醒激情","more":"more-a-link","lists":[{"text":"a","link":"a-link"}]},{"title":"h2","more":"more-a-link","lists":[{"text":"b","link":"b-link"}]}]')
					.text.text-2.fadeInDown.animated.delay-4
						img(src="#{url}img/listpage-text-2.png")
						.title 
						.btn.btn-2(next-page=".page-content",data-content='[{"title":"唤醒满足","more":"more-a-link","lists":[{"text":"a","link":"a-link"}]},{"title":"h2","more":"more-a-link","lists":[{"text":"b","link":"b-link"}]}]')
					.text.text-3.fadeInDown.animated.delay-6
						img(src="#{url}img/listpage-text-3.png")
						.title 
						.btn.btn-3(next-page=".page-content",data-content='[{"title":"唤醒视觉","more":"more-a-link","lists":[{"text":"a","link":"a-link"}]},{"title":"h2","more":"more-a-link","lists":[{"text":"b","link":"b-link"}]}]')
					.text.text-4.fadeInDown.animated.delay-8
						img(src="#{url}img/listpage-text-4.png")
						.title 
						.btn.btn-4(next-page=".page-content",data-content='[{"title":"唤醒安全","more":"more-a-link","lists":[{"text":"a","link":"a-link"}]},{"title":"h2","more":"more-a-link","lists":[{"text":"b","link":"b-link"}]}]')
		.page.page-content
			.content
				.navbar
					.btn.btn-register
						img(src="#{url}img/listpage-btn.png")
					.btn.btn-back
						img(src="#{url}img/listpage-back.png")
				.text-list
					contentx

		.page.page-register
			.bg
			.content
				.form
					//- api.giccoo.com
					register(action="http://api.giccoo.com/CX5/insert/")
