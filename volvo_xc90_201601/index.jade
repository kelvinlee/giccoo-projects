extends ../libs/jade/layout
block title
	| 沃尔沃XC90
block css
	link(rel="stylesheet",href="http://disk.giccoo.com/projects/libs/css/animate.min.css",media="none",onload="if(media!='all')media='all'")
	link(rel="stylesheet",href="css/base.css")

block content
	- var images = "http://disk.giccoo.com/projects/volvo_xc90_201601/img"
	// - var images = "img"
	.wechat-share-hide
		//- img#share(src="http://disk.giccoo.com/projects/dazhong/img/share.jpg")
	.main
		.homepage
			.bg
				img(src="#{images}/bg-1.jpg")
				a.outlink(href="http://xc90.volvocars.com.cn/")
			.bg
				img(src="#{images}/bg-2.jpg")
				.form-box
					//- .title
						img(src="#{images}/title-form.png")
					register(action="http://api.giccoo.com/volvo_xc90_201601/insert/")
			.bg.bg-3
				img(src="#{images}/bg-3.jpg")
				.sliders
					slider(myid="mainSlider",pd="true",callback="changeMain",list="#{images}/i-1.jpg,#{images}/i-2.jpg,#{images}/i-3.jpg,#{images}/i-4.jpg,#{images}/i-5.jpg")
				.left
					img(src="#{images}/move-left.png")
				.right
					img(src="#{images}/move-left.png")
			.bg.last
				img(src="#{images}/bg-4.jpg")
				.content
					//- .title
						img(src="#{images}/title.png")
					.sliders
						slider(myid="secondSlider",callback="changePoint",list="#{images}/car-1.jpg,#{images}/car-2.jpg,#{images}/car-3.jpg,#{images}/car-4.jpg,#{images}/car-5.jpg,#{images}/car-6.jpg,#{images}/car-7.jpg")
					.left2
						img(src="#{images}/m-left.png")
					.right2
						img(src="#{images}/m-left.png")
					.points
						span.on
						span
						span
						span
						span
						span
						span

		.backTop
			img(src="#{images}/back-top.png")
		//- .pop.fadeIn.animated
		//- 	.content


