							img(src="#{images}/light.png")
					a(href="javascript:void(0)",class="show-pop")
					a(href="javascript:void(0)",class="show-pop")
					a(href="javascript:void(0)",class="show-pop")
					a(href="javascript:void(0)",class="show-pop")
			.bg.bg-4
				img(src="#{images}/bg-4.jpg")
				.content
					.title
						img(src="#{images}/title.png")
					.content-slider
					.sliders
						slider(callback="changePoint",list="#{images}/car-1.jpg,#{images}/car-2.jpg,#{images}/car-3.jpg,#{images}/car-4.jpg,#{images}/car-5.jpg")
					.left
					.right
