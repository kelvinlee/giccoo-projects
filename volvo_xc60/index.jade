		//- img#share(src="http://disk.giccoo.com/projects/dazhong/img/share.jpg")
	//- .loading
		.img
			img(src="img/loading.jpg")
			.text
				span.loadtext
					em#loading-text 0
					|  %
				br
				span LOADING
	.main
		.homepage
			.bg.bg-1
				img(src="#{images}/bg-1.jpg")
				.content
					a.outlink(href="http://www.volvocars.com/zh-cn/cars/new-models/xc60")
			.bg.bg-2
				img(src="#{images}/bg-2.jpg")
				.content
					.title
						img(src="#{images}/form-title.png")
					register
						
			.bg.bg-3
				img(src="#{images}/bg-3.jpg")
				.content
					// -for(var i=1;i<6;i++)
					a(href="javascript:void(0)",class="show-pop")
