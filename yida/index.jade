extends ../libs/jade/layout-clean
block title
	| 益达


block css
	meta(name="author",content="m.giccoo.com")
	link(rel="stylesheet",href="http://disk.giccoo.com/projects/libs/css/animate.min.css",media="none",onload="if(media!='all')media='all'")
	link(rel="stylesheet",href="css/base.css")

block content
	//- - url = "/"
	.wechat-share-hide
		//- img#share(src="http://disk.giccoo.com/projects/yida/img/share.jpg")
	#loading.loading.loaded
		.logo
			img(src="#{url}yida/img/logo.png")
		.new-load
			.load-text.bounceIn.animated.delay-5
				img(src="#{url}yida/img/loading-text.png")
			.load-item.fadeInUp.animated.delay-8
				img(src="#{url}yida/img/loading-bottle-bg.png")
				.load-item-note1
					.load-move
						img(src="#{url}yida/img/loading-lang.png")
				.load-item-note2
					img(src="#{url}yida/img/loading-bottle.png")
				.load-progress
					span.n 0%
	.wrap
		begin.begin.hide
		//- build.build.hide
	.qrcode#qrcode

